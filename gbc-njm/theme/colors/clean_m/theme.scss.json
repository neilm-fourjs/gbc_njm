{
	"theme-font-family": "'\"Open Sans Condensed\",\"Droid Sans\", sans-serif'",
	"theme-font-size-ratio": "0.6",
	"theme-margin-ratio": "0.5",
	"theme-field-height-ratio": "0.6",
	"theme-radiobutton-size": "12px",
	"theme-checkbox-size": "12px",

	"palette-primary": "$mt-blue-grey",
	"palette-secondary": "$mt-grey",
	"palette-text-primary": "$mt-white",
	"palette-text-secondary": "$mt-black",
	"theme-primary-background-color": "$palette-primary-700",
	"theme-primary-emphasis-background-color": "$palette-primary-500",
	"theme-primary-faded-background-color": "$palette-primary-100",
	"theme-secondary-color": "$palette-text-secondary-100",

	"theme-sidebar-always-visible-min-width" : "999999px"
}
